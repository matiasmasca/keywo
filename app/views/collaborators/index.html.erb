<p id="notice"><%= notice %></p>

<h1>Collaborators</h1>

<table>
  <thead>
    <tr>
      <th>Disponibilidad</th>
      <th>City</th>
      <th>Zone</th>
      <th>Mobilidad</th>
      <th>Herramientas</th>
      <th>Skill</th>
      <th>Others skills</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collaborators.each do |collaborator| %>
      <tr>
        <td><%= collaborator.disponibilidad %></td>
        <td><%= collaborator.city %></td>
        <td><%= collaborator.zone %></td>
        <td><%= collaborator.mobilidad %></td>
        <td><%= collaborator.herramientas %></td>
        <td><%= collaborator.skill %></td>
        <td><%= collaborator.others_skills %></td>
        <td><%= link_to 'Show', collaborator %></td>
        <td><%= link_to 'Edit', edit_collaborator_path(collaborator) %></td>
        <td><%= link_to 'Destroy', collaborator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collaborator', new_collaborator_path %>
