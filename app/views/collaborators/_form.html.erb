<%= form_with(model: collaborator, local: true) do |form| %>
  <% if collaborator.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(collaborator.errors.count, "error") %> prohibited this collaborator from being saved:</h2>

      <ul>
      <% collaborator.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :disponibilidad %>
    <%= form.number_field :disponibilidad, id: :collaborator_disponibilidad %>
