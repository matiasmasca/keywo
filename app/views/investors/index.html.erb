<p id="notice"><%= notice %></p>

<h1>Investors</h1>

<table>
  <thead>
    <tr>
      <th>Sector</th>
      <th>Monto minimo</th>
      <th>Monto maximo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investors.each do |investor| %>
      <tr>
        <td><%= investor.sector %></td>
        <td><%= investor.monto_minimo %></td>
        <td><%= investor.monto_maximo %></td>
        <td><%= link_to 'Show', investor %></td>
        <td><%= link_to 'Edit', edit_investor_path(investor) %></td>
        <td><%= link_to 'Destroy', investor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investor', new_investor_path %>
