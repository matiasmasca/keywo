<p id="notice"><%= notice %></p>

<h1>Investor Projects</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Investor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investor_projects.each do |investor_project| %>
      <tr>
        <td><%= investor_project.project %></td>
        <td><%= investor_project.investor %></td>
        <td><%= link_to 'Show', investor_project %></td>
        <td><%= link_to 'Edit', edit_investor_project_path(investor_project) %></td>
        <td><%= link_to 'Destroy', investor_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investor Project', new_investor_project_path %>
